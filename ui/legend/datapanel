var about = require("users/dawnschumacher/nasa-eej:ui/legend/panels/about");
var univariate_data = require("users/dawnschumacher/nasa-eej:ui/legend/panels/univariate-data");
var bivariate_data = require("users/dawnschumacher/nasa-eej:ui/legend/panels/bivariate-data");

var style = require("users/dawnschumacher/nasa-eej:ui/legend/datapanel-style");

var datapanel = ui.Panel({
  style: style.panel
});

function change_section(section_index) {
  datapanel.clear();
  
  // Add the corresponding UI elements depending on which tab is pressed.
  switch(section_index) {
    case 0:
      datapanel.add(about.object);
  }
}

exports.object = datapanel;

exports.change_section = change_section;