var style = require("users/dawnschumacher/nasa-eej:ui/legend/datapanel-style");

var datapanel = ui.Panel();
var about = ui.Panel();

// About Section
about.add(ui.Label({
  value: "Where the Grass Grows Greener",
  style: style.h1
}));

about.add(ui.Label({
  value: "The Impacts of Urban Greening on Housing Prices and Neighborhood Stability",
  style: style.h2
}));

// Initialize datapanel with About section
datapanel.add(about);

function change_section(section_index) {
  datapanel.clear();
  
  switch(section_index) {
    case 0:
      datapanel.add(about);
  }
}

exports.object = datapanel;
exports.about = about;

exports.change_section = change_section;