              '2_1': {fillColor: '#a5e8cdFF'},
              '3_1': {fillColor: '#40dba7FF'},
              '1_NA': {fillColor: '#FFFFFF77'},
              '2_NA': {fillColor: '#FFFFFF77'},
              '3_NA': {fillColor: '#FFFFFF77'}
              });

