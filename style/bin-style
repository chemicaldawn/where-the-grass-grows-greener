var size = 12;
var stroke = 1;
var padding = 4;

var pip = (size + padding) / 4;
var cross = size * 3 + padding * 3 + stroke * 4;

exports.get_square = function(color) {
  return {
    backgroundColor: color,
    
    width: size.toString() + "px",
    height: size.toString() + "px",
    margin: padding.toString() + "px"
  }
}