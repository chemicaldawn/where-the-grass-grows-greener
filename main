var mapmodule = require("users/dawnschumacher/nasa-eej:ui/map");
var legendmodule = require("users/dawnschumacher/nasa-eej:ui/legend");

var map = mapmodule.get_map();
var legend = legendmodule.get_legend();

// init function, run on first load.
function init() {
  
  // Replaces the base map with a cleaner, custom map.
  ui.root.clear();
  ui.root.add(map); 
  
  // Adds the legend sidebar.
  ui.root.insert(1, legend);
}

init();