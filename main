var map = require("users/dawnschumacher/nasa-eej:ui/map");
var legend = require("users/dawnschumacher/nasa-eej:ui/legend/legend");

// init function, run on first load.
function init() {
  
  // Replaces the base map with a cleaner, custom map.
  ui.root.clear();
  ui.root.add(map.object); 
  
  // Adds the legend sidebar.
  ui.root.insert(1, legend.object);
}

function draw() {
  legend.object.clear();
  legend.construct();
}

init();